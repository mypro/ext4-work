<?xml version="1.0"?> 
<!DOCTYPE hibernate-mapping PUBLIC  
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
 
<hibernate-mapping package="cn.net.dbi.database.model" schema="DB_BASE"> 
 
    <class name="DatabaseModel" table="T_BASE_DB"> 
        <id name="uuid" type="java.lang.String" column="uuid"> 
            <generator class="assigned"/> 
        </id>
        <property name="keyword" column="keyword" type="java.lang.String" />
        <property name="name" column="name" type="java.lang.String" />
        <property name="parentUuid" column="parentUuid" type="java.lang.String" />
    </class> 
 
</hibernate-mapping> 