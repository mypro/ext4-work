<?xml version="1.0"?> 
<!DOCTYPE hibernate-mapping PUBLIC  
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
 
<hibernate-mapping package="cn.net.dbi.table.model" schema="DB_BASE"> 
 
    <class name="TableModel" table="T_BASE_TABLE"> 
        <id name="uuid" type="java.lang.String" column="uuid"> 
            <generator class="assigned"/> 
        </id>
        <property name="setUuid" column="setUuid" type="java.lang.String" />
        <property name="keyword" column="keyword" type="java.lang.String" />
        <property name="name" column="name" type="java.lang.String" />
        <property name="type" column="type" type="java.lang.Integer" />
        <property name="modifyTime" column="modifyTime" type="java.util.Date" />
    </class> 
 
</hibernate-mapping> 