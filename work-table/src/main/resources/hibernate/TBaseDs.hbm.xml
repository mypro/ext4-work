<?xml version="1.0"?> 
<!DOCTYPE hibernate-mapping PUBLIC  
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
 
<hibernate-mapping package="cn.net.dbi.datasource.model" schema="DB_BASE"> 
 
    <class name="DatasourceModel" table="T_BASE_TABLESET"> 
        <id name="uuid" type="java.lang.String" column="uuid"> 
            <generator class="assigned"/> 
        </id>
        <property name="driver" column="driver" type="java.lang.String" />
        <property name="name" column="name" type="java.lang.String" />
        <property name="url" column="url" type="java.lang.String" />
        <property name="password" column="password" type="java.lang.String" />
        <property name="createTime" column="createTime" type="java.util.Date" />
    </class> 
 
</hibernate-mapping> 