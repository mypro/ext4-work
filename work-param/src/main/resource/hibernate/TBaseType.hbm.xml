<?xml version="1.0"?> 
<!DOCTYPE hibernate-mapping PUBLIC  
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
 
<hibernate-mapping package="cn.net.dbi.param.bean" schema="DB_BASE"> 
 
    <class name="Type" table="T_BASE_TYPE"> 
        <id name="uuid" type="java.lang.String" column="uuid"> 
            <generator class="assigned"/> 
        </id>
        <property name="description" column="description" type="java.lang.String" />
        <property name="format" column="format" type="java.lang.String" />
        <property name="type" column="type" type="java.lang.Integer" />
    </class> 
 
</hibernate-mapping> 